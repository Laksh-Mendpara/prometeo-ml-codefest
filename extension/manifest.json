{
    "manifest_version": 3,
    "name": "My Web Extension",
    "version": "1.0",
    "permissions": ["storage", "activeTab", "scripting"],
    "host_permissions": ["http://localhost:3000/*"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
        "16": "/assets/cat1.jpeg"
      }
    }
  }
  